<%= render '/error_output', object: @bander %>
<h1>Bander Login</h1>
<%= form_tag login_path do %>
    <label>Name: </label>
    <%= text_field_tag :name, value = nil, :required => true %><br>
    <label>Password: </label> 
    <%= password_field_tag :password, value = nil, :required => true %><br>
    <%= submit_tag "Log In"%>
<% end %>
<%= link_to 'Log in with Google', '/auth/google_oauth2' %>